<h1><%= @user.nickname %>'s Profile</h1>
<h4><%= @user.email %></h4>

<div class="row">
  <div class="col-md-8">
    <h3><%= @user.nickname %>'s Apps</h3>
    <ul>
      <% RegisteredApplication.where(user: @user).each do |regapp| %>
        <li><%= link_to regapp.name, regapp %></li>
      <% end %>
    </ul>
  </div>
  <div class="col-md-4">
    <%= link_to "Register New App", new_registered_application_path, class: 'btn btn-primary' %>
  </div>
</div>
