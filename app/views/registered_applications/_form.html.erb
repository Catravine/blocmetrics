<%= form_for @registered_application do |f| %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control', placeholder: "Your app's name" %>
  </div>

  <div class="form-group">
    <%= f.label :url %>
    <%= f.text_field :url, class: 'form-control', placeholder: "http://sample.app" %>
  </div>

  <div class="form-group">
    <%= f.submit "Save", class: 'btn btn-success'%>
  </div>

<% end %>
